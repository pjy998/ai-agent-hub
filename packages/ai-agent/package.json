{
  "name": "ai-agent-vscode",
  "displayName": "AI Agent Hub",
  "description": "AI Agent Hub VS Code Extension",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "chatParticipants": [
      {
        "id": "ai-agent.coding",
        "name": "coding",
        "description": "AI coding assistant"
      },
      {
        "id": "ai-agent.refactor",
        "name": "refactor",
        "description": "Code refactoring assistant"
      },
      {
        "id": "ai-agent.requirements",
        "name": "requirements",
        "description": "Requirements analysis assistant"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.0"
  }
}