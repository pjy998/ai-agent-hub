{
  "name": "ai-agent-vscode",
  "displayName": "AI Agent Hub",
  "description": "AI-powered development assistant with intelligent code analysis, project scanning, and automated optimization suggestions",
  "version": "0.0.27",
  "publisher": "ai-agent-hub",
  "type": "module",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other",
    "Machine Learning",
    "Snippets",
    "Formatters"
  ],
  "keywords": [
    "ai",
    "assistant",
    "code-analysis",
    "project-scanning",
    "optimization",
    "intelligent",
    "automation",
    "development",
    "productivity",
    "chatgpt",
    "openai",
    "claude",
    "anthropic"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-agent-hub.scanProject",
        "title": "AI Agent: Scan Project",
        "category": "AI Agent Hub"
      },

      {
        "command": "ai-agent-hub.analyzeCode",
        "title": "AI Agent: Analyze Code",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.generateReport",
        "title": "AI Agent: Generate Analysis Report",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.showTransparencyReport",
        "title": "AI Agent: Show Transparency Report",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.exportInteractionData",
        "title": "AI Agent: Export Interaction Data",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.probeTokens",
        "title": "AI Agent: Probe Tokens",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.clearCache",
        "title": "AI Agent: Clear Cache",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.showSettings",
        "title": "AI Agent: Show Settings",
        "category": "AI Agent Hub"
      }
    ],
    "configuration": {
      "title": "AI Agent Hub",
      "properties": {
        "ai-agent-hub.enableOptimization": {
          "type": "boolean",
          "default": true,
          "description": "Enable performance optimization features"
        },
        "ai-agent-hub.optimization.enableCache": {
          "type": "boolean",
          "default": true,
          "description": "Enable caching for better performance"
        },
        "ai-agent-hub.optimization.cacheMaxSize": {
          "type": "number",
          "default": 1000,
          "description": "Maximum number of cache entries"
        },
        "ai-agent-hub.optimization.cacheTTL": {
          "type": "number",
          "default": 1800000,
          "description": "Cache time-to-live in milliseconds (30 minutes)"
        },
        "ai-agent-hub.optimization.enableBatching": {
          "type": "boolean",
          "default": true,
          "description": "Enable batch processing for AI requests"
        },
        "ai-agent-hub.optimization.batchSize": {
          "type": "number",
          "default": 5,
          "description": "Number of requests to batch together"
        },
        "ai-agent-hub.optimization.batchTimeout": {
          "type": "number",
          "default": 2000,
          "description": "Batch timeout in milliseconds"
        },
        "ai-agent-hub.optimization.maxRetries": {
          "type": "number",
          "default": 3,
          "description": "Maximum number of retries for failed requests"
        },
        "ai-agent-hub.optimization.retryDelay": {
          "type": "number",
          "default": 1000,
          "description": "Delay between retries in milliseconds"
        },
        "ai-agent-hub.optimization.enableRateLimiting": {
          "type": "boolean",
          "default": true,
          "description": "Enable rate limiting for AI requests"
        },
        "ai-agent-hub.optimization.requestsPerMinute": {
          "type": "number",
          "default": 60,
          "description": "Maximum requests per minute"
        },
        "ai-agent-hub.performance.memory.heapWarning": {
          "type": "number",
          "default": 200,
          "description": "Heap memory warning threshold in MB"
        },
        "ai-agent-hub.performance.memory.heapCritical": {
          "type": "number",
          "default": 400,
          "description": "Heap memory critical threshold in MB"
        },
        "ai-agent-hub.performance.memory.rssWarning": {
          "type": "number",
          "default": 500,
          "description": "RSS memory warning threshold in MB"
        },
        "ai-agent-hub.performance.memory.rssCritical": {
          "type": "number",
          "default": 800,
          "description": "RSS memory critical threshold in MB"
        },
        "ai-agent-hub.performance.timing.fileProcessWarning": {
          "type": "number",
          "default": 1000,
          "description": "File processing warning threshold in milliseconds"
        },
        "ai-agent-hub.performance.timing.aiResponseWarning": {
          "type": "number",
          "default": 5000,
          "description": "AI response warning threshold in milliseconds"
        },
        "ai-agent-hub.performance.cache.hitRateWarning": {
          "type": "number",
          "default": 0.7,
          "description": "Cache hit rate warning threshold (0.0-1.0)"
        },
        "ai-agent-hub.aiProvider": {
          "type": "string",
          "enum": [
            "openai",
            "anthropic",
            "azure-openai",
            "vscode-lm",
            "local"
          ],
          "default": "openai",
          "description": "AI provider to use for requests"
        },
        "ai-agent-hub.openai.apiKey": {
          "type": "string",
          "default": "",
          "description": "OpenAI API key"
        },
        "ai-agent-hub.openai.model": {
          "type": "string",
          "default": "gpt-4",
          "description": "OpenAI model to use"
        },
        "ai-agent-hub.anthropic.apiKey": {
          "type": "string",
          "default": "",
          "description": "Anthropic API key"
        },
        "ai-agent-hub.anthropic.model": {
          "type": "string",
          "default": "claude-3-sonnet-20240229",
          "description": "Anthropic model to use"
        }
      }
    },
    "chatParticipants": [
      {
        "id": "ai-agent-hub",
        "name": "ai-agent-hub",
        "description": "AI Agent Hub - Intelligent development assistant with project analysis and optimization capabilities",
        "isSticky": true
      },
      {
        "id": "scan",
        "name": "scan",
        "description": "Project scanner for comprehensive codebase analysis - 项目扫描器，用于全面的代码库分析",
        "isSticky": true
      },
      {
        "id": "analyze",
        "name": "analyze",
        "description": "Code analyzer for intelligent insights and suggestions - 代码分析器，提供智能洞察和建议",
        "isSticky": true
      },
      {
        "id": "optimize",
        "name": "optimize",
        "description": "Performance optimizer for code and project improvements - 性能优化器，用于代码和项目改进",
        "isSticky": true
      },
      {
        "id": "recommend",
        "name": "recommend",
        "description": "Intelligent recommendation system for project improvements - 智能推荐系统，提供项目改进建议",
        "isSticky": true
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "jest",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "quality:check": "npm run lint && npm run test",
    "quality:fix": "npm run lint:fix && npm run test",
    "quality:full": "npm run lint && npm run test && npm run type-check",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "ci:check": "npm run type-check && npm run format:check && npm run lint && npm run test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "dependencies": {
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.0.5",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "typescript": "^4.9.0"
  }
}
