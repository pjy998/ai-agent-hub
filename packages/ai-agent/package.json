{
  "name": "ai-agent-vscode",
  "displayName": "AI Agent Hub",
  "description": "AI-powered coding assistant with project analysis, C# coding standards, and intelligent workflow automation for VS Code",
  "version": "0.0.22",
  "publisher": "jieky",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages",
    "Linters",
    "Other"
  ],
  "keywords": [
    "ai",
    "assistant",
    "coding",
    "analysis",
    "csharp",
    "standards",
    "refactor",
    "copilot",
    "mcp",
    "automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pjy998/ai-agent-hub.git"
  },
  "bugs": {
    "url": "https://github.com/pjy998/ai-agent-hub/issues"
  },
  "homepage": "https://github.com/pjy998/ai-agent-hub#readme",
  "license": "MIT",
  "icon": "icon.png",
  "galleryBanner": {
    "color": "#1e1e1e",
    "theme": "dark"
  },
  "activationEvents": [
    "onStartupFinished",
    "onLanguage"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ai-agent-hub.analyzeSelf",
        "title": "AI Agent Hub: Analyze Self",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.generateReport",
        "title": "AI Agent Hub: Generate Analysis Report",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent-hub.viewRecommendations",
        "title": "AI Agent Hub: View Recommendations",
        "category": "AI Agent Hub"
      },
      {
        "command": "ai-agent.tokenProbe",
        "title": "AI Agent: Token Probe",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.tokenProbeQuick",
        "title": "AI Agent: Token Probe (Quick Test)",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.tokenProbeHistory",
        "title": "AI Agent: Token Probe History",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.tokenProbeClearHistory",
        "title": "AI Agent: Clear Token Probe History",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.improvedTokenProbe",
        "title": "AI Agent: Improved Token Probe",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.quickTokenProbe",
        "title": "AI Agent: Quick Token Probe",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.customTokenProbe",
        "title": "AI Agent: Custom Token Probe",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.showTokenStats",
        "title": "AI Agent: Show Token Usage Statistics",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.exportTokenReport",
        "title": "AI Agent: Export Token Usage Report",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.analyzeCSharpProject",
        "title": "AI Agent: Analyze C# Project",
        "category": "AI Agent"
      },
      {
        "command": "ai-agent.analyzeCSharpFolder",
        "title": "AI Agent: Analyze C# Folder",
        "category": "AI Agent"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "ai-agent-hub.analyzeSelf",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent-hub.generateReport",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent-hub.viewRecommendations",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent.tokenProbe",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent.tokenProbeQuick",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent.tokenProbeHistory"
        },
        {
          "command": "ai-agent.tokenProbeClearHistory"
        },
        {
          "command": "ai-agent.improvedTokenProbe",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent.quickTokenProbe",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent.customTokenProbe",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent.showTokenStats"
        },
        {
          "command": "ai-agent.exportTokenReport"
        },
        {
          "command": "ai-agent.analyzeCSharpProject",
          "when": "workspaceFolderCount > 0"
        },
        {
          "command": "ai-agent.analyzeCSharpFolder",
          "when": "workspaceFolderCount > 0"
        }
      ]
    },
    "chatParticipants": [
      {
        "id": "ai-agent.analyze",
        "name": "analyze",
        "description": "Project self-analysis assistant"
      },
      {
        "id": "ai-agent.config",
        "name": "config",
        "description": "Dynamic configuration generator for multi-language analysis"
      },
      {
        "id": "ai-agent.token",
        "name": "token",
        "description": "Token limit testing and analysis for GitHub Copilot Chat models - 测试和分析模型Token限制"
      },
      {
          "id": "ai-agent.code",
          "name": "code",
          "description": "Multi-language code analysis and quality assessment assistant - 多语言代码分析和质量评估助手"
      },
      {
        "id": "ai-agent.report",
        "name": "report",
        "description": "Project report generator with multiple formats - 项目报告生成器，支持多种格式"
      },
      {
        "id": "ai-agent.recommend",
        "name": "recommend",
        "description": "Intelligent recommendation system for project improvements - 智能推荐系统，提供项目改进建议",
        "isSticky": true
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "build": "npm run compile",
    "package": "vsce package",
    "publish": "vsce publish",
    "vscode:prepublish": "npm run build"
  },
  "dependencies": {
    "tiktoken": "^1.0.22"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.0"
  }
}